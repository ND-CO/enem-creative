<!-- Hero -->
<div class="wrap container-fluid">
  <div class="container">
    <div class="hero">
      <div class="row content-hero">
        <div class="col-md-6 col-sm-6 padfixleft border-overflow">
          <div class="borderFour"></div>
          <h1 class="hero-one header-fade"> We are are a design and marketing duo in the windy city.</h1>
        </div>
        <div class="col-md-6 col-sm-6 view-work padfixright border-overflow">
          <div class="borderTwo"></div>
            <div class="view">
              <h5 class="hero-two viewprojects">View Work</h5>
              <div class="borderView view-work"></div>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>

